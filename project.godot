; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="РобоКод"
config/version="0.1.0"
run/main_scene="res://scenes/menu/menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://assets/logo/logo.svg"

[autoload]

Strings="*res://values/strings.gd"
Dimens="*res://values/dimens.gd"
SceneSwitcher="*res://scenes/switcher.gd"
Signal_Debugger="*res://addons/SignalVisualizer/Debugger/SignalDebugger.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="La Curse De Godot"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/SignalVisualizer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/find-everywhere/plugin.cfg", "res://addons/format_on_save/plugin.cfg", "res://addons/gd-plug-ui/plugin.cfg", "res://addons/log/plugin.cfg", "res://addons/project_time_tracker/plugin.cfg", "res://addons/script-tabs/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://scenes/": "teal"
}

[filesystem]

import/blender/enabled=false

[layer_names]

2d_physics/layer_1="Environment"
2d_physics/layer_2="Player"

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/defaults/default_clear_color=Color(0.168627, 0.176471, 0.188235, 1)
